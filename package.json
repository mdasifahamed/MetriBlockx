    "ioredis": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^22.19.9",
    "dotenv": "^17.2.3",
    "prisma": "^6.19.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
