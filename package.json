  "version": "1.0.0",
  "description": "Blockchain data fetcher with ethers.js v6",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:eth": "tsx watch src/index.ts --chain=ethereum",
